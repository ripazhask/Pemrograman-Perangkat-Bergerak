<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:paddingTop="@dimen/activity_vertical_layout"
    android:paddingBottom="@dimen/activity_vertical_layout"
    android:paddingLeft="@dimen/activity_horizontal_layout"
    android:paddingRight="@dimen/activity_horizontal_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/btn_move_activity"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/activity_vertical_layout"
        android:text="Move Activity"/>
    <Button
        android:id="@+id/btn_move_data"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/activity_vertical_layout"
        android:text="Move Activity with Data"/>
    <Button
        android:id="@+id/BtnDial"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/activity_vertical_layout"
        android:text="Lakukan Panggilan"/>
    <Button
        android:id="@+id/Btn_inet"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:text="Website Polines"/>
    <Button
        android:id="@+id/BtnTest"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:text="Belajar Intent APP"/>

</LinearLayout>